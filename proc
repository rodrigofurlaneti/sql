-- =============================================
CREATE PROCEDURE [dbo].[de_stp_ContratoServicoVinculado_consultaIdConTip] 
	(
		@id_contip		INT,
		@id_serv_vinc	INT

	)
AS
BEGIN
	select cont.id_contServVinc
		from de_tb_contrato_servicovinculado cont
	where 
	id_contip = @id_contip
	AND
	id_serv_vinc = @id_serv_vinc

END
