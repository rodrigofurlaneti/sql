-- =============================================
-- Author:		<Rodrigo>
-- Create date: <24/08/2017>
-- Description:	<Alterar a tabela contrato tipo servico>
-- =============================================
CREATE PROCEDURE [dbo].[de_stp_ContratoTipoServico_alterar] 
	(	
		@id_contip		INT,
		@id_con			INT,
		@id_cent_serv	INT
	)
AS
BEGIN
	SET NOCOUNT ON;
	UPDATE de_tb_contrato_tiposervico SET
			id_con			=	@id_con,
			id_cent_serv	=	@id_cent_serv
	where
			id_contip	=	@id_contip
END

GO




